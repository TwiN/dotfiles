URxvt.termName: rxvt-unicode-256color

URxvt.background: black
URxvt.foreground: white

URxvt.font:     xft:Inconsolata:size=11,xft:SauceCodePro\ Nerd\ Font\ Mono:size=11
URxvt.boldFont: xft:Inconsolata:size=11,xft:SauceCodePro\ Nerd\ Font\ Mono:size=11:style=bold
URxvt.scrollBar: false
URxvt.saveLines: 75000
URxvt.internalBorder: 8 
URxvt*transparent: true
URxvt*shading: 12
URxvt*visualBell: true

! Colors (Theme: Gruvbox)
*.color0: #1d2021
*.color1: #cc241d
*.color2: #98971a
*.color3: #d79921
*.color4: #458588
*.color5: #b16286
*.color6: #689d6a
*.color7: #a89984
*.color8: #928374
*.color9: #fb4934
*.color10: #b8bb26
*.color11: #fabd2f
*.color12: #83a598
*.color13: #d3869b
*.color14: #8ec07c
*.color15: #ebdbb2

Xft.dpi: 128

! URxvt plugins
URxvt.perl-ext-common:  default,matcher,resize-font,selection-to-clipboard

! Clickable URL
URxvt.url-launcher: /usr/bin/firefox-developer-edition
URxvt.matcher.button: 1

! Resize font
URxvt.keysym.C-Down:  resize-font:smaller
URxvt.keysym.C-Up:    resize-font:bigger
